<script type="text/javascript">
	$(document).ready(function(){
		myBusinessCards.init();
	});
	
	var myBusinessCards = new function(){
		this.init = function(){
			$(document).on(touchOrClick,'.BOM_Delete',function(){
				var Obj = $(this).parents('.Business_One');
				timconfirm("<?php echo $this->_Lang_general['confirm'];?>","<?php echo $this->_Lang_general['confirm_delete'];?>",function(){
					$.ajax({
						type: "POST",
						url: __AjaxURL__+"?ajaxProcess",
						data: "menu=deleteBusiness&bID="+Obj.data('bid'),
						success: function(d){
							var res = $.parseJSON(d);
							if(res.ack == 'success')
							{
								Obj.slideUp(100,function(){
									$(this).remove();
									
								});
								
							}
						}
					});
					
					
					
				});
			});
		};
	};
</script>
